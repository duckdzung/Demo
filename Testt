mvn clean install -Dmaven.wagon.http.ssl.insecure=true -Dmaven.wagon.http.ssl.allowall=true


<settings>
    <servers>
        <server>
            <id>ignore-ssl</id>
            <configuration>
                <httpConfiguration>
                    <all>
                        <useSslInsecureMode>true</useSslInsecureMode>
                    </all>
                </httpConfiguration>
            </configuration>
        </server>
    </servers>
</settings>
